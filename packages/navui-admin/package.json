{
  "name": "@library/navui-admin",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "types": "build/index.d.ts",
  "module": "build/index.js",
  "main": "build/index.umd.js",
  "scripts": {
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "test": "jest --config jest.config.js",
    "build:components": "node ./nav-cli/src/commands/build.js",
    "build:components:dts": "vue-tsc --declaration --emitDeclarationOnly",
    "build:lib": "yarn build:components && cp package.json build && cp README.md build",
    "cli:dev": "esbuild --bundle ./nav-ui/src/index.js --format=cjs --platform=node --outfile=./lib/index.cjs --watch",
    "cli:build": "esbuild --bundle ./nav-ui/src/index.js --format=cjs --platform=node --outfile=./lib/index.cjs",
    "cli": "node ./lib/index.cjs create"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@vueuse/core": "^9.12.0",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.6.3",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/test-utils": "^2.2.7",
    "@vuedx/typescript-plugin-vue": "^0.7.6",
    "babel-jest": "^29.3.1",
    "child_process": "^1.0.2",
    "commander": "^9.4.1",
    "esbuild": "^0.16.12",
    "fs-extra": "^11.1.0",
    "inquirer": "^9.1.4",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "kolorist": "^1.6.0",
    "sass": "^1.57.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-svg-loader": "^2.2.0",
    "vitepress": "1.0.0-alpha.45",
    "vitepress-demoblock": "^1.0.4",
    "vitepress-theme-demoblock": "^2.0.2",
    "vue-tsc": "^1.0.11"
  },
  "license": "MIT"
}
